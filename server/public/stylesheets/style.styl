html
    // height:100%;
body
    // height:100%;
    // padding-top:150px
.fb-logo
    float:right;
    margin:4px;
.ki-modal
    clear:both;
.app-history
    a
        display:block;
        padding:5px;
        &:hover 
            background:#f2f2f2;
.fb-title
    padding:10px;
    // background:url('/images/logo.png') 100% 10px no-repeat;
    h1
        font-family: 'Lucida Grande', 'Lucida Sans Unicode', Helvetica, Arial, Verdana, sans-serif;
        font-size:54px;
        line-height:50px;
        margin-bottom:10px;
        font-weight:normal;
    .path
        color:#BBB;
        margin-top:-10px;
        font-size:10px;
    a
        color:#135653;
.fb-page 
    margin:10px 0;
    position: relative;
    h3
        background:#29c2bd;
        border: 2px solid #000;
        padding:10px;
        color: #fff;
        border-radius:5px 5px 0 0;
        
    .options
        position: absolute;
        padding:10px;
        top:0;
        right:0;
        color:#fff;
    .status
        margin:0 10px;
        display:inline-block;
.form-page-add
    border:2px #999 dashed;
    text-align:center;
    padding:20px;
    margin-top:30px;
.version
    border:#999 2px solid;
    // border-radius:0 0 5px 5px;
    border-top: 0;
    // margin-left: 50px;
    position: relative;
    background:#d3e9f4;
    color:#3898be;
    padding:10px;
    a.version_no
        font-size:16px;
    .options
        padding:5px;
        color: #666;

.versions
    list-style: none;
    margin:0;


.version-add
    float:left;
    // margin-left: 80px;

ul.timestamps
    list-style:none;
    margin:20px 0;
    li
        padding:10px;
        border-bottom:1px solid #FFF;
        background:#fff;
        &:hover
            background:#f2f2f2;
        border-top: 1px solid #EEE;
        position:relative;
    .build-version 
        color:#AAA;
        margin-left: 20px;
    .build-time
        position:absolute;
        font-size:12px;
        color:#666;
        right:10px;
        top:10px;

.buildto
    .status
        display:inline-block;
        margin:0 10px;
.buildto-version
    text-align:right;
.ks-popup
.ks-overlay
    position:absolute;
    left: -9999px;
.timestamp
    cursor:pointer;
    &:hover
        background:#ffff99;
    
.footer
    margin-top: 80px;
    text-align:right;
    a
        color: black;
@import "reports.styl";